    <EditText android:id="@+id/edt_register_1_password" android:layout_width="match_parent" android:layout_height="match_parent" android:background="@drawable/edt_general_normal" android:digits="0123456789abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ" android:hint="@string/activity_register_1_edt_password" android:inputType="textVisiblePassword" android:paddingLeft="204px" android:textColor="#333333" android:textColorHint="#bab9b9" android:textSize="15sp"/>  
