    </RelativeLayout> 
  </RelativeLayout>  
  <TextView android:layout_width="966px" android:layout_height="wrap_content" android:layout_gravity="center_horizontal" android:layout_marginTop="25px" android:text="@string/activity_register_1_tv_tip_1" android:textColor="#888888" android:textSize="12sp"/>  
